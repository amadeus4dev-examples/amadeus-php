{
  "name": "amadeus4dev/amadeus-php",
  "description": "PHP library for the Amadeus Self-Service APIs",
  "license": "MIT",
  "authors": [
    {
      "name": "Amadeus for Developers",
      "email": "developers@amadeus.com"
    }
  ],
  "require": {
    "php": ">=7.4.0",
    "ext-json": "*",
    "ext-curl": "*"
  },
  "autoload": {
    "psr-4": {
      "Amadeus\\": "src/",
      "Amadeus\\Airport\\": "src/airport",
      "Amadeus\\Booking\\": "src/booking",
      "Amadeus\\Client\\": "src/client",
      "Amadeus\\Exceptions\\": "src/exceptions",
      "Amadeus\\ReferenceData\\": "src/referenceData",
      "Amadeus\\ReferenceData\\Locations\\": "src/referenceData/locations",
      "Amadeus\\ReferenceData\\Locations\\Hotels\\": "src/referenceData/locations/hotels",
      "Amadeus\\Resources\\": "src/resources",
      "Amadeus\\Shopping\\": "src/shopping",
      "Amadeus\\Shopping\\Availability\\": "src/shopping/availability",
      "Amadeus\\Shopping\\FlightOffers\\": "src/shopping/flightOffers",
      "Amadeus\\Travel\\": "src/travel",
      "Amadeus\\Travel\\Predictions\\": "src/travel/predictions"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5.19",
    "phpstan/phpstan": "^1.5.2",
    "friendsofphp/php-cs-fixer": "^3.8"
  },
  "autoload-dev": {
    "psr-4": {
      "Amadeus\\Tests\\": "tests/",
      "Amadeus\\Tests\\Airport\\": "tests/airport",
      "Amadeus\\Tests\\Booking\\": "tests/booking",
      "Amadeus\\Tests\\Client\\": "tests/client",
      "Amadeus\\Tests\\Exceptions\\": "tests/exceptions",
      "Amadeus\\Tests\\ReferenceData\\": "tests/referenceData",
      "Amadeus\\Tests\\ReferenceData\\Locations\\": "tests/referenceData/locations",
      "Amadeus\\Tests\\ReferenceData\\Locations\\Hotels\\": "tests/referenceData/locations/hotels",
      "Amadeus\\Tests\\Resources\\": "tests/resources",
      "Amadeus\\Tests\\Shopping\\": "tests/shopping",
      "Amadeus\\Tests\\Shopping\\Availability\\": "tests/shopping/availability",
      "Amadeus\\Tests\\Shopping\\FlightOffers\\": "tests/shopping/flightOffers",
      "Amadeus\\Tests\\Travel\\": "tests/travel",
      "Amadeus\\Tests\\Travel\\Predictions\\": "tests/travel/predictions"
    }
  }
}
